{
    "name": "cane-kanbanboard",
    "version": "0.0.1",
    "author": {
        "name": "Derek Donnelly",
        "email": "derek@derekdonnelly.com",
        "url": "http://derekdonnelly.com"
    },
    "description": "A single page kanban board application using Couchbase, AngularJS, NodeJS and ExpressJS... or the 'CANE' stack!",
    "homepage": "https://github.com/derekdon/cane-kanbanboard",
    "repository": {
        "type": "git",
        "url": "git@github.com:derekdon/cane-kanbanboard.git"
    },
    "private": true,
    "main": "./app/app.js",
    "devDependencies": {
        "grunt": "~0.4.4",
        "grunt-contrib-jshint": "~0.10.0",
        "grunt-ngmin": "0.0.3",
        "grunt-contrib-uglify": "~0.4.0",
        "grunt-contrib-watch": "~0.6.1",
        "grunt-contrib-clean": "~0.5.0",
        "grunt-contrib-less": "~0.11.0",
        "grunt-protractor-runner": "~0.2.4",
        "grunt-browserify": "~2.1.0",
        "debowerify": "~0.7.1",
        "browserify-shim": "~3.5.0"
    },
    "licenses": {
        "type": "MIT",
        "url": "https://raw.github.com/ngbp/ngbp/master/LICENSE"
    },
    "browser": {
        "jquery": "./vendor/jquery/dist/jquery.min.js",
        "bootstrap": "./vendor/bootstrap/dist/js/bootstrap.min.js",
        "underscore.string": "./vendor/underscore.string/dist/underscore.string.min.js"
    },
    "browserify-shim": {
        "jquery": "$",
        "bootstrap": {
            "depends": ["jquery:$"]
        },
        "underscore.string": {
            "exports": "_str",
            "depends": [ "underscore" ]
        }
    },
    "browserify": {
        "transform": ["browserify-shim", "debowerify"]
    }
}